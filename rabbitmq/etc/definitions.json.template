{
 "rabbit_version": "4.0.8",
  "default_vhost": "/",
  "default_user": "${AMQP_USERNAME}",
  "default_pass": "${AMQP_PASSWORD}",
  "users": [
    {
      "name": "${AMQP_USERNAME}",
      "password": "${AMQP_PASSWORD}",
      "tags": ["administrator"]
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "${AMQP_USERNAME}",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "parameters": [],
  "global_parameters": [
    {
      "name": "internal_cluster_id",
      "value": "rabbitmq-cluster-id-1"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "ha",
      "pattern": ".*",
      "priority": 1,
      "definition": {
        "delivery-limit": 3,
        "max-length-bytes": 536870912,
        "overflow": "reject-publish"
      }
    }
  ],
  "aten": {
    "poll_interval": 10000,
    "poll_timeout": 10000,
    "detection_timeout": 15000
  },
  "queues":[
    {
      "name":"${AMQP_QUEUE_NAME}",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "quorum"
      }
    }
  ],
  "exchanges":[
    {
      "name": "npl_notifications",
      "vhost": "/",
      "type": "fanout",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings":[
    {
      "source":"npl_notifications",
      "vhost":"/",
      "destination":"${AMQP_QUEUE_NAME}",
      "destination_type":"queue",
      "arguments":{}
    }
  ]
}
